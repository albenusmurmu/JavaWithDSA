import java.util.*;

public class Inputs {
    public static void main(String[] args){
        Scanner st = new Scanner(System.in);
        System.out.println("Enter Value Of a");
        int a = st.nextInt();
        System.out.println("Enter value of b");
        int b = st.nextInt();
        int c = a*b;
        System.out.println("Product of a*b = "+c);
    }
}
